<h2><%= @board.title %><h2>


<table class="table">
  <thead>
    <th>Title</th>
    <th>User</th>
    <th>Time</th>
  </thead>


  <tbody>
    <% @topics.each do |topic|%>
    <tr>
      <td><%= link_to(topic.title, topic_path(topic)) %></td>
      <td><%= topic.user.email %></td>
      <td><%= topic.created_at %></td>
    </tr>
    <%end%>
  </tbody>
</table>




